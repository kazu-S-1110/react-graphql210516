export const SEARCH_AND_EMPLOYEE = gql`
  query($name: String, $joinYear: Int, $dept: String) {
    allEmployees(
      name_Icontains: $name
      joinYear: $joinYear
      department_DeptName_Icontains: $dept
    ) {
      edges {
        node {
          id
          name
          joinYear
          department {
            id
            deptName
          }
        }
      }
    }
  }
`;