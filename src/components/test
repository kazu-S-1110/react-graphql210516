export const PAGINATE_MORE_EMPLOYEE = gql`
  query($first: Int, $after: String) {
    allDepartments(first: $first, after: $after) {
      edges {
        cursor
        node {
          id
          deptName
        }
      }
      pageInfo {
        startCursor
        endCursor
        hasNextPage
      }
    }
  }
`;